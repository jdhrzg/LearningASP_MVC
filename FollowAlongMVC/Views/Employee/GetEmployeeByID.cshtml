@model FollowAlongMVC.Models.Employee

@{
    ViewBag.Title = "Employee By ID";
}

<h2>Employee Details</h2>

<table>
    <tr>
        <td>
            <b>Id:</b>
        </td>
        <td>
            @Model.ID
        </td>
    </tr>
    <tr>
        <td>
            <b>Name:</b>
        </td>
        <td>
            @Model.Name
        </td>
    </tr>
    <tr>
        <td>
            <b>City:</b>
        </td>
        <td>
            @Model.City
        </td>
    </tr>
</table>